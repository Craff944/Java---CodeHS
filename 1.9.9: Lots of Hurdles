public class HurdlesKarel extends SuperKarel
{
    public void run()
    {
        for(int i = 0; i < 5; i++) 
         {
             movetoHurdle();
            jumpHurdle();
           }    
    }
    private void jumpHurdle()
    {
        turnLeft();
         move();
         turnRight();
        move();
         turnRight();
         move();
        turnLeft();
        
    }
    private void movetoHurdle()
    {
        move();
        move();
        
    }
}
