public class PancakeKarel extends Karel
{
    //"Defining a method"
    private void makePancakes()
    {
        putBall();
        putBall();
        putBall();
    }
    public void run()
    {
        move();
        // "Calling a method"
        makePancakes();
        move();
        move();
        makePancakes();
        move();
        move();
        makePancakes();
        move();
        
    }
}
